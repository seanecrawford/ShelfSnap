platform :ios, '15.0'

target 'ShelfSnapIOS' do
  # Pods for object detection.  MLÂ Kit is distributed via CocoaPods.
  # See https://firebase.google.com/docs/ml-kit/ios/detect-objects for details.
  use_frameworks!

  pod 'GoogleMLKit/ObjectDetection'
